# Google Cloud Platform Configuration
GCP_PROJECT_ID=your-gcp-project-id

# M3 Backend Configuration
M3_BACKEND=sqlite
M3_PROJECT_ID=${GCP_PROJECT_ID}

# OAuth2 Configuration (Optional)
M3_OAUTH2_ENABLED=false
M3_OAUTH2_ISSUER_URL=https://your-auth-provider.com
M3_OAUTH2_AUDIENCE=m3-api
M3_OAUTH2_SCOPES=read:mimic-data

# Docker Configuration
COMPOSE_PROJECT_NAME=m3